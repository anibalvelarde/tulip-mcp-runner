# Tulip MCP Server Configuration
# Required: Tulip API credentials
TULIP_API_KEY=<your-tulip-api-key>
TULIP_API_SECRET=<your-tulip-api-secret>
TULIP_BASE_URL=https://<your-tulip-saas-instance>.tulip.co

# Optional Parameters:
TULIP_WORKSPACE_ID=DEFAULT
MCP_MAX_RETRIES=3
MCP_BASE_DELAY=1000
ENABLED_TOOLS=machine,read-only
MCP_SERVER_NAME=tulip-mcp
MCP_SERVER_VERSION=1.0.0
MCP_DEBUG=false